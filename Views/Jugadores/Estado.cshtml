@model List<string> 
<div class="container">
    <div class="col-sm-6">
        <h3>Lista de Jugadores por estado</h3>
    </div>
    <div class="col-sm-6">
        <a class="btn btn-success d-block" asp-action="Index">Volver</a>
    </div>

</div>
<div class="row">
    @if (Model.Count() > 0)
    {
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <td>Jugadores-estados</td>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item</td>
                    </tr>
                }
            </tbody>
        </table>
    }
    else
    {
        <p>No existe ningun Jugador</p>
    }
</div>